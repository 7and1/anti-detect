{
  "name": "anti-detect",
  "version": "1.0.0",
  "private": true,
  "description": "The Ultimate Browser Fingerprint Scanner",
  "keywords": [
    "fingerprint",
    "privacy",
    "anti-detect",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anti-detect/anti-detect.com"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "prepare": "husky",
    "deploy:api": "turbo deploy --filter=@anti-detect/api",
    "deploy:web": "turbo deploy --filter=@anti-detect/web",
    "deploy": "turbo deploy",
    "db:migrate": "pnpm --filter=@anti-detect/api db:migrate",
    "db:seed": "pnpm --filter=@anti-detect/api db:seed"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hono/zod-validator": "0.7.5",
    "@next/bundle-analyzer": "^15.5.6"
  }
}
